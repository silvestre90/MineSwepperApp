<AnchorPane prefHeight="218.0" prefWidth="391.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="startPageController">
   <children>
      <ComboBox fx:id="cmbDifficulty" editable="true" layoutX="133.0" layoutY="53.0" prefHeight="25.0" prefWidth="190.0" promptText="Open List" />
      <ComboBox fx:id="cmbGrid" editable="true" layoutX="133.0" layoutY="100.0" prefHeight="25.0" prefWidth="190.0" promptText="Open List" />
      <Button fx:id="btnStart" layoutX="133.0" layoutY="148.0" mnemonicParsing="false" onAction="#startMinesweeper" prefHeight="25.0" prefWidth="90.0" text="Start Game" />
      <Button fx:id="btnExit" layoutX="234.0" layoutY="148.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="25.0" prefWidth="90.0" text="Exit" />
      <Label layoutX="14.0" layoutY="57.0" prefHeight="17.0" prefWidth="101.0" text="Choose Difficulty:" />
      <Label layoutX="14.0" layoutY="104.0" prefHeight="17.0" prefWidth="101.0" text="Grid Size:" />
      <Label alignment="CENTER" layoutX="100.0" layoutY="21.0" prefHeight="17.0" prefWidth="150.0" text="Minesweeper" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblError" layoutX="9.0" layoutY="140.0" prefHeight="62.0" prefWidth="123.0" />
      <Button fx:id="btnResults" layoutX="133.0" layoutY="181.0" mnemonicParsing="false" onAction="#showBestResults" prefHeight="25.0" prefWidth="190.0" text="Show Best Results" />
   </children>
</AnchorPane>
